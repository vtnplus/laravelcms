{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "illuminate/mail": "5.1.*",
        "intervention/image": "2.*",
        "maatwebsite/excel": "~2.1.0",
        "laravel/socialite": "~2.0",
        "ignited/laravel-omnipay": "2.*",
        "vlucas/phpdotenv": "~1.0"
        
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Remios\\": "remios/",
            "Remios\\Apps\\" : "remios/app/Controllers",
            "Remios\\Models\\" : "remios/app/Models",
            "Modules\\": "modules/",
            "Themes\\": "contents/templates/",
            "Themes\\_default\\": "contents/templates/default/",
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
        "files": [
            "remios/helpers.php"
        ]
    },
    
    "config": {
        "preferred-install": "dist"
    }
}
